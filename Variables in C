#include<stdio.h>
#include<stdlib.h>

int main() { 

    char myName[] = "John";
    int myAge = 30;

    // \n means next line 
    // % means special format 
    // %s sub string into the sentence 
    // vice versa w/ other variable types

    // sub myName into %s
    printf("Hello %s \n", myName);

    // print my age
    printf("My %s is %d", "age", myAge);
    return 0;
}
